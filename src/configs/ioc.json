{
    "secret:manager": {
        "target": "SecretManager",
        "type": "class",
        "lifetime": "singleton",
        "path": "../../../modules/secret/services",
        "args": [
            {
                "type": "mdb",
                "cloud": {
                    "region": "us-east-1",
                    "accessKeyId": "AWS_ACCESS_KEY_ID",
                    "secretAccessKey": "AWS_SECRET_ACCESS_KEY"
                },
                "mdb": {
                    "enabled": true,
                    "database": "kozen",
                    "collection": "secrets",
                    "uri": "MDB_URI"
                }
            }
        ],
        "dependencies": [
            {
                "key": "assistant",
                "target": "IoC",
                "type": "ref"
            },
            {
                "key": "logger",
                "target": "logger:service",
                "type": "ref"
            }
        ]
    },
    "secret:manager:aws": {
        "target": "SecretManagerAWS",
        "type": "class",
        "lifetime": "singleton",
        "path": "../../../modules/secret/services",
        "args": [
            null
        ],
        "dependencies": [
            {
                "key": "assistant",
                "target": "IoC",
                "type": "ref"
            },
            {
                "key": "logger",
                "target": "logger:service",
                "type": "ref"
            }
        ]
    },
    "secret:manager:mdb": {
        "target": "SecretManagerMDB",
        "type": "class",
        "lifetime": "singleton",
        "path": "../../../modules/secret/services",
        "args": [
            null
        ],
        "dependencies": [
            {
                "key": "assistant",
                "target": "IoC",
                "type": "ref"
            },
            {
                "key": "logger",
                "target": "logger:service",
                "type": "ref"
            }
        ]
    }
}